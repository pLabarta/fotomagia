Suggestion 10

    gp.arg("--set-config capturemode=0");
    gp.arg("--set-config imageformat=0");

    let mut child = gp.spawn()?;

    let status = child.wait()?;

    println!("child status was: {}", status);
    println!("gp output was: {}", gp.output()?);

